#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5f4cabd02550 .scope module, "state_machine_sim" "state_machine_sim" 2 3;
 .timescale -9 -9;
v0x5f4cabd2a120_0 .var "clk", 0 0;
v0x5f4cabd2a1e0_0 .net "cs", 1 0, v0x5f4cabd29d00_0;  1 drivers
v0x5f4cabd2a2b0_0 .var "halt", 0 0;
v0x5f4cabd2a3b0_0 .var "reset", 0 0;
v0x5f4cabd2a480_0 .var "run", 0 0;
S_0x5f4cabcda490 .scope module, "state_machine1" "state_machine" 2 7, 3 5 0, S_0x5f4cabd02550;
 .timescale -9 -9;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "run";
    .port_info 2 /INPUT 1 "halt";
    .port_info 3 /INPUT 1 "reset";
    .port_info 4 /OUTPUT 2 "cs";
v0x5f4cabcda690_0 .net "clk", 0 0, v0x5f4cabd2a120_0;  1 drivers
v0x5f4cabd29d00_0 .var "cs", 1 0;
v0x5f4cabd29de0_0 .net "halt", 0 0, v0x5f4cabd2a2b0_0;  1 drivers
v0x5f4cabd29eb0_0 .net "reset", 0 0, v0x5f4cabd2a3b0_0;  1 drivers
v0x5f4cabd29f70_0 .net "run", 0 0, v0x5f4cabd2a480_0;  1 drivers
E_0x5f4cabd17900/0 .event negedge, v0x5f4cabd29eb0_0;
E_0x5f4cabd17900/1 .event posedge, v0x5f4cabcda690_0;
E_0x5f4cabd17900 .event/or E_0x5f4cabd17900/0, E_0x5f4cabd17900/1;
    .scope S_0x5f4cabcda490;
T_0 ;
    %wait E_0x5f4cabd17900;
    %load/vec4 v0x5f4cabd29eb0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x5f4cabd29d00_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5f4cabd29d00_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 2;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 2;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 2;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %jmp T_0.5;
T_0.2 ;
    %load/vec4 v0x5f4cabd29f70_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x5f4cabd29d00_0, 0;
T_0.6 ;
    %jmp T_0.5;
T_0.3 ;
    %pushi/vec4 2, 0, 2;
    %assign/vec4 v0x5f4cabd29d00_0, 0;
    %jmp T_0.5;
T_0.4 ;
    %load/vec4 v0x5f4cabd29de0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.8, 8;
    %pushi/vec4 0, 0, 2;
    %assign/vec4 v0x5f4cabd29d00_0, 0;
    %jmp T_0.9;
T_0.8 ;
    %pushi/vec4 1, 0, 2;
    %assign/vec4 v0x5f4cabd29d00_0, 0;
T_0.9 ;
    %jmp T_0.5;
T_0.5 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5f4cabd02550;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4cabd2a120_0, 0, 1;
T_1.0 ;
    %delay 50, 0;
    %load/vec4 v0x5f4cabd2a120_0;
    %inv;
    %store/vec4 v0x5f4cabd2a120_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5f4cabd02550;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4cabd2a480_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4cabd2a2b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4cabd2a3b0_0, 0, 1;
    %delay 1, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f4cabd2a3b0_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f4cabd2a480_0, 0, 1;
    %delay 200, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4cabd2a480_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f4cabd2a2b0_0, 0, 1;
    %delay 500, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4cabd2a2b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5f4cabd2a480_0, 0, 1;
    %delay 1000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5f4cabd2a3b0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 23 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5f4cabd02550;
T_3 ;
    %vpi_call 2 27 "$monitor", "clk = %d, run = %d, halt = %d, reset = %d, cs = %d", v0x5f4cabd2a120_0, v0x5f4cabd2a480_0, v0x5f4cabd2a2b0_0, v0x5f4cabd2a3b0_0, v0x5f4cabd2a1e0_0 {0 0 0};
    %vpi_call 2 28 "$dumpfile", "state_machine_sim.vcd" {0 0 0};
    %vpi_call 2 29 "$dumpvars" {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "state_machine_sim.v";
    "state_machine.v";
